{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://franceschini.wedding/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "franceschiniwedding.com"
        }
      ]
    }
  ],
  "crons": [{
    "path": "/api/cron",
    "schedule": "0 0 * * *"
  }]
}